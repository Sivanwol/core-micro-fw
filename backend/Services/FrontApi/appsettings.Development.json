{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "http://localhost:5082",
          "http://localhost:5081",
          "http://localhost:5083"
        ],
        "ValidIssuer": "application-api-security"
      }
    }
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Local Front Api Health Check",
        "Uri": "http://localhost:5082/healthz"
      }
    ],
    "Webhooks": [],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
